  "differential": {
    "element": [
      {
        "id": "Measure",
        "path": "Measure"
      },
      {
        "id": "Measure.extension:scoringUnit",
        "path": "Measure.extension",
        "sliceName": "scoringUnit",
        "min": 0,
        "max": "1",
        "type": [
          {
            "code": "Extension",
            "profile": [
              "http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-scoringUnit"
            ]
          }
        ],
        "mustSupport": true
      },
      {
        "id": "Measure.relatedArtifact",
        "path": "Measure.relatedArtifact",
        "slicing": {
          "discriminator": [
            {
              "type": "value",
              "path": "type"
            }
          ],
          "rules": "open"
        },
        "min": 0,
        "max": "*",
        "mustSupport": true
      },
      {
        "id": "Measure.relatedArtifact:component",
        "path": "Measure.relatedArtifact",
        "sliceName": "component",
        "min": 0,
        "max": "*",
        "mustSupport": true
      },
      {
        "id": "Measure.relatedArtifact:component.extension:groupId",
        "path": "Measure.relatedArtifact.extension",
        "sliceName": "groupId",
        "min": 0,
        "max": "1",
        "type": [
          {
            "code": "Extension",
            "profile": [
              "http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-groupId"
            ]
          }
        ],
        "mustSupport": true
      },
      {
        "id": "Measure.relatedArtifact:component.extension:weight",
        "path": "Measure.relatedArtifact.extension",
        "sliceName": "weight",
        "min": 0,
        "max": "1",
        "type": [
          {
            "code": "Extension",
            "profile": [
              "http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-weight"
            ]
          }
        ],
        "mustSupport": true
      },
      {
        "id": "Measure.relatedArtifact:component.type",
        "path": "Measure.relatedArtifact.type",
        "short": "Component measure",
        "definition": "A component of the composite measure.",
        "min": 1,
        "max": "1",
        "type": [
          {
            "code": "code"
          }
        ],
        "fixedCode": "composed-of",
        "mustSupport": true
      },
      {
        "id": "Measure.relatedArtifact:component.display",
        "path": "Measure.relatedArtifact.display",
        "min": 0,
        "max": "1",
        "mustSupport": true
      },
      {
        "id": "Measure.relatedArtifact:component.resource",
        "path": "Measure.relatedArtifact.resource",
        "min": 1,
        "max": "1",
        "type": [
          {
            "code": "canonical",
            "targetProfile": [
              "http://hl7.org/fhir/StructureDefinition/Measure"
            ]
          }
        ],
        "mustSupport": true
      },
      {
        "id": "Measure.scoring",
        "path": "Measure.scoring",
		"short": "composite",
        "type": [
          {
            "code": "CodeableConcept"
          }
        ],
        "patternCodeableConcept": {
          "coding": [
            {
              "system": "http://terminology.hl7.org/CodeSystem/measure-scoring",
              "code": "composite"
            }
          ]
        },
        "mustSupport": true,
        "binding": {
          "strength": "extensible",
          "valueSet": "http://terminology.hl7.org/ValueSet/measure-scoring"
        }
      },
      {
        "id": "Measure.compositeScoring",
        "path": "Measure.compositeScoring",
        "mustSupport": true,
        "binding": {
          "strength": "extensible",
          "valueSet": "http://terminology.hl7.org/ValueSet/composite-measure-scoring"
        }
      },
      {
        "id": "Measure.group",
        "path": "Measure.group",
        "constraint" : [
        {
          "key" : "cmp-10",
          "severity" : "error",
          "human" : "Composite measures may not specify population criteria",
          "expression" : "extension('http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-scoring').value.coding.code contains 'composite' implies population.count() = 0"
        },
        {
          "key" : "cmp-11",
          "severity" : "error",
          "human" : "Composite measures must specify composite scoring and at least one component",
          "expression" : "extension('http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-scoring').value.coding.code contains 'composite' implies extension('http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-compositeScoring').exists() and extension('http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-component').exists()"
        }],
        "mustSupport" : true,
        "isModifier" : false,
        "isSummary" : false
      },
      {
        "id" : "Measure.group.extension:scoring",
        "path" : "Measure.group.extension",
        "sliceName" : "scoring",
        "min" : 0,
        "max" : "1",
        "type" : [
          {
            "code" : "Extension",
            "profile" : [
               "http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-scoring"
            ]
          }
        ],
        "condition" : [
          "cmp-10",
          "cmp-11"
        ],
        "mustSupport" : true
      },
      {
        "id": "Measure.group.extension:scoringUnit",
        "path": "Measure.group.extension",
        "sliceName": "scoringUnit",
        "min": 0,
        "max": "1",
        "type": [
          {
            "code": "Extension",
            "profile": [
              "http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-scoringUnit"
            ]
          }
        ],
        "mustSupport": true
      },
      {
        "id": "Measure.group.extension:compositeScoring",
        "path": "Measure.group.extension",
        "sliceName": "compositeScoring",
        "min": 0,
        "max": "1",
        "type": [
          {
            "code": "Extension",
            "profile": [
              "http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-compositeScoring"
            ]
          }
        ],
        "mustSupport": true
      },
      {
        "id" : "Measure.group.extension:component",
        "path" : "Measure.group.extension",
        "sliceName" : "component",
        "short" : "What artifact",
        "definition" : "Specifies an artifact this is a component of a composite artifact.",
        "min" : 0,
        "max" : "*",
        "base" : {
          "path" : "Element.extension",
          "min" : 0,
          "max" : "*"
        },
        "type" : [{
          "code" : "Extension",
          "profile" : ["http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-component"]
        }],
        "condition" : ["ele-1",
        "cmp-11"],
        "mustSupport" : true,
        "isModifier" : false
      },
      {
        "id": "Measure.group.extension:component.extension:groupId",
        "path": "Measure.group.extension.extension",
        "sliceName": "groupId",
        "min": 0,
        "max": "1",
        "type": [
          {
            "code": "Extension",
            "profile": [
              "http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-groupId"
            ]
          }
        ],
        "mustSupport": true
      },
      {
        "id": "Measure.group.extension:component.extension:weight",
        "path": "Measure.group.extension.extension",
        "sliceName": "weight",
        "min": 0,
        "max": "1",
        "type": [
          {
            "code": "Extension",
            "profile": [
              "http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-weight"
            ]
          }
        ],
        "mustSupport": true
      },
      {
        "id" : "Measure.group.population",
        "path" : "Measure.group.population",
        "condition" : [
          "cmp-10"
        ],
        "mustSupport" : true
      }
    ]
  }
}
