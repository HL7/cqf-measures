  "differential" : {
    "element" : [
      {
        "id" : "Measure",
        "path" : "Measure",
        "constraint" : [
          {
            "key" : "cmp-1",
            "severity" : "error",
            "human" : "Population basis must be specified at the root, or on each group",
            "expression" : "extension('http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-populationBasis').exists() or group.select(extension('http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-populationBasis').exists()).allTrue()"
          },
          {
            "key" : "cmp-2",
            "severity" : "error",
            "human" : "Scoring must either be specified at the root, or on each group",
            "expression" : "scoring.exists() xor (group.exists() and group.select(extension('http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-scoring').exists()).allTrue())"
          },
          {
            "key" : "cmp-3",
            "severity" : "error",
            "human" : "Scoring unit may only be specified at the root if the scoring is also specified at the root",
            "expression" : "extension('http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-scoringUnit').exists() implies scoring.exists()"
          },
          {
            "key" : "cmp-4",
            "severity" : "error",
            "human" : "Improvement notation must be specified for scoring types other than cohort",
			"expression" : "(scoring.exists() and (scoring.coding.code.contains('proportion') or scoring.coding.code.contains('ratio') or scoring.coding.code.contains('continuous-variable') or scoring.coding.code.contains('composite')) and improvementNotation.exists()) xor (group.exists() and group.select((extension('http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and (extension('http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-scoring').value.coding.code.contains('proportion') or extension('http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-scoring').value.coding.code.contains('ratio') or extension('http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-scoring').value.coding.code.contains('continuous-variable') or extension('http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-scoring').value.coding.code.contains('composite')) and  extension('http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-improvementNotation').exists())).allTrue()) xor (scoring.exists() and scoring.coding.code.contains('cohort'))"
          },
          {
            "key" : "cmp-5",
            "severity" : "error",
            "human" : "If type is specified at the root, it may not be specified on any group",
            "expression" : "type.exists() implies group.select(extension('http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-type').exists()).allFalse()"
          },
          {
            "key" : "cmp-6",
            "severity" : "error",
            "human" : "If riskAdjustment is specified at the root, it may not be specified on any group",
            "expression" : "riskAdjustment.exists() implies group.select(extension('http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-riskAdjustment').exists()).allFalse()"
          },
          {
            "key" : "cmp-7",
            "severity" : "error",
            "human" : "If rateAggregation is specified at the root, it may not be specified on any group",
            "expression" : "rateAggregation.exists() implies group.select(extension('http://hl7.org/fhir/uv/cqfmeasures/StructureDefinition/cqfm-rateAggregation').exists()).allFalse()"
          }
        ],
        "mustSupport" : true
