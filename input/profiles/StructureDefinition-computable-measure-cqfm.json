      {
        "id": "Measure.group",
        "path": "Measure.group",
        "constraint": [
          {
            "key": "cmp-8",
            "severity": "error",
            "human": "Scoring unit may only be specified at the root if the scoring is also specified at the root",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoringUnit').exists() implies extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists()"
          },
          {
            "key": "cmp-9",
            "severity": "error",
            "human": "Improvement notation must be specified for scoring types other than cohort",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').value in ('proportion' | 'ratio' | 'continuous-variable') implies extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-improvementNotation').exists()"
          },
          {
            "key": "cmp-10",
            "severity": "error",
            "human": "Cohort measures have one and only one population criteria and that criteria has a code of initial-population",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').value = 'cohort' implies population.count() = 1 and population.select(code.coding.where(system = 'http://terminology.hl7.org/CodeSystem/measure-popuplation').code = 'initial-population').allTrue()"
          },
          {
            "key": "cmp-11",
            "severity": "error",
            "human": "Proportion measures must have one and only one initial population",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').value = 'proportion' implies population.where(code.coding.where(system = 'http://terminology.hl7.org/CodeSystem/measure-population').code = 'initial-population').count = 1"
          },
          {
            "key": "cmp-12",
            "severity": "error",
            "human": "Proportion measures must have one and only one denominator",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').value = 'proportion' implies population.where(code.coding.where(system = 'http://terminology.hl7.org/CodeSystem/measure-population').code = 'denominator').count = 1"
          },
          {
            "key": "cmp-13",
            "severity": "error",
            "human": "Proportion measures must have one and only one numerator",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').value = 'proportion' implies population.where(code.coding.where(system = 'http://terminology.hl7.org/CodeSystem/measure-population').code = 'numerator').count = 1"
          },
          {
            "key": "cmp-14",
            "severity": "error",
            "human": "Proportion measures may have at most one denominator-exclusion",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').value = 'proportion' implies population.where(code.coding.where(system = 'http://terminology.hl7.org/CodeSystem/measure-population').code = 'denominator-exclusion').count <= 1"
          },
          {
            "key": "cmp-15",
            "severity": "error",
            "human": "Proportion measures may have at most one denominator-exception",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').value = 'proportion' implies population.where(code.coding.where(system = 'http://terminology.hl7.org/CodeSystem/measure-population').code = 'denominator-exception').count <= 1"
          },
          {
            "key": "cmp-16",
            "severity": "error",
            "human": "Proportion measures may have at most one numerator-exclusion",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').value = 'proportion' implies population.where(code.coding.where(system = 'http://terminology.hl7.org/CodeSystem/measure-population').code = 'numerator-exclusion').count <= 1"
          },
          {
            "key": "cmp-17",
            "severity": "error",
            "human": "Proportion measures may not have other than initial population, denominator, numerator, denominator-exclusion, denominator-exception, and numerator-exclusion criteria",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').value = 'proportion' implies population.select(code.coding.where(system = 'http://terminology.hl7.org/CodeSystem/measure-population').code in ('initial-population' | 'numerator' | 'denominator' | 'denominator-exclusion' | 'denominator-exception' | 'numerator-exclusion')).allTrue()"
          },
          {
            "key": "cmp-18",
            "severity": "error",
            "human": "Ratio measures must have between 1 and 2 initial population criteria",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').value = 'ratio' implies (population.where(code.coding.where(system = 'http://terminology.hl7.org/CodeSystem/measure-population').code = 'initial-population').count() in (1 | 2))"
          },
          {
            "key": "cmp-19",
            "severity": "error",
            "human": "Ratio measures must have one and only one denominator",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').value = 'ratio' implies population.where(code.coding.where(system = 'http://terminology.hl7.org/CodeSystem/measure-population').code = 'denominator').count = 1"
          },
          {
            "key": "cmp-20",
            "severity": "error",
            "human": "Ratio measures must have one and only one numerator",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').value = 'ratio' implies population.where(code.coding.where(system = 'http://terminology.hl7.org/CodeSystem/measure-population').code = 'numerator').count = 1"
          },
          {
            "key": "cmp-21",
            "severity": "error",
            "human": "Ratio measures may have at most one denominator-exclusion",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').value = 'ratio' implies population.where(code.coding.where(system = 'http://terminology.hl7.org/CodeSystem/measure-population').code = 'denominator-exclusion').count <= 1"
          },
          {
            "key": "cmp-22",
            "severity": "error",
            "human": "Ratio measures may have at most two measure-observation criteria",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').value = 'ratio' implies population.where(code.coding.where(system = 'http://terminology.hl7.org/CodeSystem/measure-population').code = 'measure-observation').count <= 2"
          },
          {
            "key": "cmp-23",
            "severity": "error",
            "human": "Ratio measures may have at most one numerator-exclusion",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').value = 'ratio' implies population.where(code.coding.where(system = 'http://terminology.hl7.org/CodeSystem/measure-population').code = 'numerator-exclusion').count <= 1"
          },
          {
            "key": "cmp-24",
            "severity": "error",
            "human": "Ratio measures may not have other than initial population, denominator, numerator, denominator-exclusion, numerator-exclusion, and measure-observation criteria",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').value = 'ratio' implies population.select(code.coding.where(system = 'http://terminology.hl7.org/CodeSystem/measure-population').code in ('initial-population' | 'numerator' | 'denominator' | 'denominator-exclusion' | 'measure-observation' | 'numerator-exclusion')).allTrue()"
          },
          {
            "key": "cmp-25",
            "severity": "error",
            "human": "Continuous variable measures must have one and only one initial population criteria",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').value = 'continuous-variable' implies population.where(code.coding.where(system = 'http://terminology.hl7.org/CodeSystem/measure-population').code = 'initial-population').count = 1"
          },
          {
            "key": "cmp-26",
            "severity": "error",
            "human": "Continuous variable measures must have one and only one measure population",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').value = 'continuous-variable' implies population.where(code.coding.where(system = 'http://terminology.hl7.org/CodeSystem/measure-population').code = 'measure-population').count = 1"
          },
          {
            "key": "cmp-27",
            "severity": "error",
            "human": "Continuous variable measures must have one and only one measure observation",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').value = 'continuous-variable' implies population.where(code.coding.where(system = 'http://terminology.hl7.org/CodeSystem/measure-population').code = 'measure-observation').count = 1"
          },
          {
            "key": "cmp-28",
            "severity": "error",
            "human": "Continuous variable measures may have at most one measure population exclusion",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').value = 'continuous-variable' implies population.where(code.coding.where(system = 'http://terminology.hl7.org/CodeSystem/measure-population').code = 'measure-population-exclusion').count <= 1"
          },
          {
            "key": "cmp-29",
            "severity": "error",
            "human": "Continuous variable measures may not have other than initial population, measure-population, measure-population-exclusion, and measure-observation criteria",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').value = 'ratio' implies population.select(code.coding.where(system = 'http://terminology.hl7.org/CodeSystem/measure-population').code in ('initial-population' | 'measure-population' | 'meausre-population-exclusion' | 'measure-observation')).allTrue()"
          },
          {
            "key": "cmp-30",
            "severity": "error",
            "human": "Composite measures may not specify population criteria",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').value = 'composite' implies population.count() = 0"
          },
          {
            "key": "cmp-31",
            "severity": "error",
            "human": "Composite measures must specify composite scoring and at least one component",
            "expression": "extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-scoring').value = 'composite' implies extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-compositeScoring').exists() and extension('http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-component').exists()"
          }
        ],
        "mustSupport": true
      },
