      </extension>
      <extension url="statement">
         <valueString value="define &quot;SDE Payer&quot;:&#xa;    [Coverage: type in &quot;Payer&quot;] Payer&#xa;      return {&#xa;        code: Payer.type,&#xa;        period: Payer.period&#xa;      }"/>
