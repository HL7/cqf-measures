            </extension>
            <extension url="statement">
               <valueString value="define &quot;Stratifier 1&quot; :
  &quot;Inpatient Encounter&quot; Encounter
    where not (PrincipalDiagnosis(Encounter).code in &quot;Psychiatric/Mental Health Patient&quot;)"></valueString>
